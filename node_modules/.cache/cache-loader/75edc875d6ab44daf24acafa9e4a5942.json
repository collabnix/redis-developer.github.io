eact from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'index-from-source',\n\ttitle: 'Create Redis database from Source',\n\tsidebar_label: 'Redis from Source',\n\tslug: '/create/from-source/'\n};\nexport const metadata = {\n  \"unversionedId\": \"create/from-source/index-from-source\",\n  \"id\": \"create/from-source/index-from-source\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Create Redis database from Source\",\n  \"description\": \"Step 1: Download, extract and compile Redis\",\n  \"source\": \"@site/docs/create/from-source/index-from-source.mdx\",\n  \"slug\": \"/create/from-source/\",\n  \"permalink\": \"/create/from-source/\",\n  \"editUrl\": \"https://github.com/redis-developer/redis-developer/edit/master/docs/create/from-source/index-from-source.mdx\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"Redis from Source\",\n  \"sidebar\": \"docs\",\n  \"previous\": {\n    \"title\": \"Create Redis database on Microsoft Windows 10\",\n    \"permalink\": \"/create/windows\"\n  },\n  \"next\": {\n    \"title\": \"Develop your application using programming languages\",\n    \"permalink\": \"/develop\"\n  }\n};\n  /* @jsxRuntime classic */\n/* @jsx mdx */\n\nexport const toc = [{\n  value: 'Step 1: Download, extract and compile Redis',\n  id: 'step-1-download-extract-and-compile-redis',\n  children: []\n}, {\n  value: 'Step 2: Running Redis Server',\n  id: 'step-2-running-redis-server',\n  children: []\n}, {\n  value: 'Step 3: Interacting with Redis Client',\n  id: 'step-3-interacting-with-redis-client',\n  children: []\n}, {\n  value: 'Next Step',\n  id: 'next-step',\n  children: []\n}];\n\nconst layoutProps = {\n  toc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h3 {...{\n      \"id\": \"step-1-download-extract-and-compile-redis\"\n    }}>{`Step 1: Download, extract and compile Redis`}</h3>\n    <pre><code parentName=\"pre\" {...{}}>{`wget http://download.redis.io/redis-stable.tar.gz\ntar xvzf redis-stable.tar.gz\ncd redis-stable\nmake\n`}</code></pre>\n    <p>{`It is a good idea to copy both the Redis server and the command line interface into the proper places, either manually using the following commands:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`sudo cp src/redis-server /usr/local/bin/\nsudo cp src/redis-cli /usr/local/bin/\n`}</code></pre>\n    <p>{`Or just using `}<inlineCode parentName=\"p\">{`sudo make install.`}</inlineCode></p>\n    <p>{`The binaries that are now compiled are available in the src directory. `}</p>\n    <h3 {...{\n      \"id\": \"step-2-running-redis-server\"\n    }}>{`Step 2: Running Redis Server`}</h3>\n    <pre><code parentName=\"pre\" {...{}}>{`$ redis-server\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"step-3-interacting-with-redis-client\"\n    }}>{`Step 3: Interacting with Redis Client`}</h3>\n    <pre><code parentName=\"pre\" {...{}}>{`$ src/redis-cli\nredis> set foo bar\nOK\nredis> get foo\n\"bar\"\n`}</code></pre>\n    <p>{`Please note: By default, Redis has 0-15 indexes for databases, you can change that number databases NUMBER in redis.conf.`}</p>\n    <h3 {...{\n      \"id\": \"next-step\"\n    }}>{`Next Step`}</h3>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/explore/redisinsight\"\n        }}>{`Connect to Redis database using RedisInsight`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/develop/java\"\n        }}>{`Develop with Java and Redis`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/develop/python\"\n        }}>{`Develop with Python and Redis`}</a></li>\n    </ul>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}