CAAL,CA7CK,CAqDL,aAAG,UAAI,UAAU,CAAC,GAAf,EAAqB,iEAArB,CAAsF,kBAAY,UAAU,CAAC,IAAvB,EAA8B,IAA9B,CAAtF,CAAuI,aAAvI,CAAH,CArDK,CAAP,CAuDD,CAED,CACAD,UAAU,CAACG,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'index-manage',\n\ttitle: '5. Manage Index',\n\tsidebar_label: '5. Manage Index',\n\tslug: '/howtos/moviesdatabase/manage'\n};\nexport const metadata = {\n  \"unversionedId\": \"howtos/moviesdatabase/manage/index-manage\",\n  \"id\": \"howtos/moviesdatabase/manage/index-manage\",\n  \"isDocsHomePage\": false,\n  \"title\": \"5. Manage Index\",\n  \"description\": \"Listing and inspecting the indexes\",\n  \"source\": \"@site/docs/howtos/moviesdatabase/manage/index.manage.mdx\",\n  \"slug\": \"/howtos/moviesdatabase/manage\",\n  \"permalink\": \"/howtos/moviesdatabase/manage\",\n  \"editUrl\": \"https://github.com/redis-developer/redis-developer/edit/master/docs/howtos/moviesdatabase/manage/index.manage.mdx\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"5. Manage Index\",\n  \"sidebar\": \"docs\",\n  \"previous\": {\n    \"title\": \"4. Query Data\",\n    \"permalink\": \"/howtos/moviesdatabase/query\"\n  },\n  \"next\": {\n    \"title\": \"6. Import datasets\",\n    \"permalink\": \"/howtos/moviesdatabase/import\"\n  }\n};\n  /* @jsxRuntime classic */\n/* @jsx mdx */\n\nexport const toc = [{\n  value: 'Listing and inspecting the indexes',\n  id: 'listing-and-inspecting-the-indexes',\n  children: []\n}, {\n  value: 'Updating your Indexing',\n  id: 'updating-your-indexing',\n  children: []\n}, {\n  value: 'Dropping the Index',\n  id: 'dropping-the-index',\n  children: []\n}];\n\nconst layoutProps = {\n  toc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h3 {...{\n      \"id\": \"listing-and-inspecting-the-indexes\"\n    }}>{`Listing and inspecting the indexes`}</h3>\n    <p>{`The `}<inlineCode parentName=\"p\">{`FT._LIST`}</inlineCode>{` command provides a list of all RediSearch indexes in your database:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`> FT._LIST\n1) \"idx:movie\"\n`}</code></pre>\n    <p><inlineCode parentName=\"p\">{`FT.INFO`}</inlineCode>{` provides information about a specific index:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`> FT.INFO \"idx:movie\" \n \n 1) \"index_name\"\n 2) \"idx:movie\"\n ...\n 5) \"index_definition\"\n ...\n 7) \"fields\"\n ...\n9) \"num_docs\"\n10) \"4\" \n...\n\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"updating-your-indexing\"\n    }}>{`Updating your Indexing`}</h3>\n    <p>{`As you are build your application and add more information to the database you may need to add new fields to the index. The `}<inlineCode parentName=\"p\">{`FT.ALTER`}</inlineCode>{` command enables you to do this.`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`> FT.ALTER idx:movie SCHEMA ADD plot TEXT WEIGHT 0.5\n\"OK\"\n`}</code></pre>\n    <p>{`The `}<inlineCode parentName=\"p\">{`WEIGHT`}</inlineCode>{` declares the importance of this field when calculating result accuracy. This is a multiplication factor (default is 1); so in this example the plot is less important than the title.`}</p>\n    <p>{`Let's do a query with the new indexed field:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`> FT.SEARCH idx:movie \"empire @genre:{Action}\" RETURN 2 title plot\n\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"dropping-the-index\"\n    }}>{`Dropping the Index`}</h3>\n    <p>{`You can drop an index using the `}<inlineCode parentName=\"p\">{`FT.DROPINDEX`}</inlineCode>{` command.`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`> FT.DROPINDEX idx:movie\n\n\"OK\"\n`}</code></pre>\n    <p>{`Dropping an index does not impact the indexed hashes, this means that the movies are still inside the database.`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`>SCAN 0 MATCH movie:*\n\n1) \"0\"\n2) 1) \"movie:11002\"\n   2) \"movie:11004\"\n   3) \"movie:11003\"\n   4) \"movie:11005\"\n`}</code></pre>\n    <p><em parentName=\"p\">{`Note: You can delete the indexed document/hashes by adding the `}<inlineCode parentName=\"em\">{`DD`}</inlineCode>{` parameter.`}</em></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}